{% extends 'base.html.twig' %}

{% block title "Admin Panel" %}

{% block body %}

    <h1>Here all the users</h1>


    <table class="table table-bordered">
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Cin</th>
            <th>Telephone</th>
            <th>Actions</th>
        </tr>
        {% for user in users %}
        <tr>
            <td>{{ user.username }}</td>
            <td>{{ user.email }}</td>
            <td>{{ user.cIN }}</td>
            <td>{{ user.telephone }}</td>
        </tr>
        {% endfor %}
    </table>

{% endblock %}